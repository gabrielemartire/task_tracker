<p style="color: green"><%= notice %></p>

<% content_for :title, "Projects" %>

<div>
  <%= link_to "Back to landing", root_path %>
  <%= link_to "New project", new_project_path %>
</div>

<h1>Projects</h1>

<div id="projects">
  <% @projects.each do |project| %>
    <div class="element">
      <%= render project %>
      <p>
        <%= link_to "Show this project", project %>
      </p>
    </div>
  <% end %>
</div>

